{
    "name": "igrejanet/support",
    "description": "Support Classes for any project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Lopes Santos",
            "email": "fale_com_lopez@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "nesbot/carbon": "^2.0",
        "jansenfelipe/utils": "^2.0",
        "illuminate/support": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "Igrejanet\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "extra" : {
        "laravel": {
            "providers": [
                "Igrejanet\\Support\\ServiceProviders\\SupportServiceProvider"
            ]
        }
    }
}
